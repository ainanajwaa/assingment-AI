    "import pyttsx3\n",
    " \n",
    " \n",
    "engine = pyttsx3.init()\n",
    " \n",
    "engine.say(\"welcome to our group project which is speech recognition \")\n",
    "engine.setProperty('rate', 120)\n",
    "engine.setProperty('volume', 0.9)\n",
    "engine.runAndWait()"
   ]
